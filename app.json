{
  "expo": {
    "name": "PrimeVideoUIClone",
    "slug": "PrimeVideoUIClone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/icon.png",
    "splash": {
      "image": "./src/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f171e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./src/images/favicon.png"
    },
    "description": "This is just a React Native learning experience for me. I just did the Home and Find pages UI, i might do the rest if i find the time. Feel free to fork and continue if you want.",
    "githubUrl": "https://github.com/Alissonsleal/PrimeVideoUIClone"
  }
}
